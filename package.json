  "scripts": {
    "start": "echo try client instead of start",
    "build": "REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_HASH=$(git rev-parse HEAD) react-scripts build",
    "build:aws": "REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_HASH=$(cat commit-id) react-scripts build",
    "client": "PORT=3000 REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_HASH=$(git rev-parse HEAD) react-scripts start",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js src/ server/",
    "format": "prettier --write \"{src,server}/**/*.js\"",
    "server": "babel-node ./server/index.js",
    "server:dev": "nodemon --watch server --watch src/domain --ignore '*.test.js' --exec npm run server",
    "prod": "REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_HASH=$(git rev-parse HEAD) npm run server",
    "prod:aws": "REACT_APP_VERSION=$npm_package_version REACT_APP_GIT_HASH=$(cat commit-id) npm run server --migrate",
    "sync": "babel-node --presets es2015,stage-2 --plugins transform-object-rest-spread ./server/sync/cli.js",
    "test": "react-scripts test --env=jsdom",
    "test:once": "CI=true npm run test",
    "test:sync": "jest ./server/sync/tests/core.test.js"
  },
  "browserslist": [
